{
  "name": "frontend",
  "version": "0.0.1",
  "description": "The new MemberMatters frontend",
  "productName": "MemberMatters",
  "cordovaId": "org.membermatters.app",
  "capacitorId": "",
  "author": "Jaimyn Mayer <hello@jaimyn.com.au>",
  "private": true,
  "scripts": {
    "dev": "quasar dev -m spa",
    "dev-ios": "quasar dev -m capacitor -T ios ",
    "dev-android": "quasar dev -m capacitor -T android ",
    "build": "quasar build -m spa",
    "build-pwa": "quasar build -m pwa",
    "build-ios": "cd src-capacitor && npx cap sync && npx cap open ios",
    "build-android": "cd src-capacitor && npx cap sync && npx cap open android",
    "build-electron": "quasar build -m electron",
    "lint": "eslint --ext .js,.vue src"
  },
  "dependencies": {
    "@capacitor/core": "^1.5.1",
    "@fortawesome/fontawesome-pro": "^5.12.1",
    "@quasar/extras": "^1.0.0",
    "axios": "^0.18.1",
    "http-proxy": "^1.18.0",
    "jsrsasign": "^8.0.12",
    "moment": "^2.24.0",
    "qrcode": "^1.4.4",
    "quasar": "^1.9.11",
    "vue-i18n": "^8.0.0",
    "vue2-transitions": "^0.3.0",
    "vuex": "^3.1.3",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@quasar/app": "^1.0.0",
    "@quasar/quasar-app-extension-icon-genie": "^1.1.3",
    "babel-eslint": "^10.0.1",
    "devtron": "^1.4.0",
    "electron": "^8.1.1",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.1.2",
    "prettier": "^1.19.1"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "resolutions": {
    "@babel/parser": "7.7.5"
  }
